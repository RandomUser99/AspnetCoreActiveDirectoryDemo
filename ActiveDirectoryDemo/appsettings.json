{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DomainPath": "LDAP://XXXX.YYY",
  "Users": {
    "LdapPath": "LDAP://XXXX.YYY/OU=Users,DC=XXXX,DC=YYY"
  },
  "Passwords": {
    "Length": 12,
    "NumberOfNonAlphanumericCharacters": 0
  },
  "Mail": {
    "SmtpHost": "SmptRelay",
    "FromAddress": "noreply@domain.com",
    "AdminAddress": "DomainAdmin@domain.com"
  }
}
